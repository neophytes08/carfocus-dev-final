@font-face {
  font-family: open-sans;
  src: url('<%= asset_path("OpenSans-Regular.ttf") %>') format('truetype');
}
html,
body {
  background-color: #fff;
  font-family: open-sans, helvetica, Arial, sans-serif;
}
body div#wrapper {
  background-color: #262B2D;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li div.user-info div.profile-pic {
  margin: 20px 0;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li div.user-info div.profile-pic img {
  width: 150px;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li div.user-info div.profile-name h4 {
  margin: 0;
  padding: 0 15px 10px;
  color: #f8f8f8;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li div.user-info div.profile-name h4 span {
  color: #CBCBCB;
  font-weight: bold;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li a {
  color: #f8f8f8;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li a:hover,
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li a:focus {
  background-color: #212124;
}
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li.active > a,
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li.active > a:hover,
body div#wrapper nav.navbar div.sidebar div.sidebar-nav ul#side-menu li.active > a:focus {
  background-color: #212124;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper {
  /*width: 85%;*/
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category {
  border: 1px solid #DDDDDD;
  border-radius: 2px;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li {
  background-color: #fff;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li a {
  color: #474747;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li:first-child a {
  border-radius: 2px 0 0 2px;
  border-right: 1px solid #ddd;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li:nth-child( 2 ) a {
  border-radius: 0;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li:last-child a {
  border-radius: 0 2px 2px 0;
  border-left: 1px solid #ddd;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li.active,
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li:hover {
  border-bottom: 2px solid #43609c;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li.active a,
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.content ul.inventory-category li:hover a {
  background-color: #f3f3f3;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.table-settings {
  margin-top: 30px;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.tab-content {
  min-height: 600px;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.expenses div.form-expenses form div.row div div div.expense-content {
  margin-top: 50px;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.expenses div.form-expenses form div.row div.purpose div div.input-purpose button {
  border-radius: 50%;
  padding: 7px 12px;
}
body div#wrapper div#page-wrapper div.container-fluid div.row div.main-wrapper div.row div div.panel div.panel-body div.form-info div.form-categories {
  margin-top: 30px;
}
body div#productModalInfo div.modal-dialog div.modal-content div.modal-body div.row div.product-description dl dt {
  width: 105px;
  text-align: left;
}
body div#productModalInfo div.modal-dialog div.modal-content div.modal-body div.row div.product-description dl dd {
  margin-left: 113px;
}
body div#productModalInfo div.modal-dialog div.modal-content div.modal-body div.row div.product-description dl dd.description {
  text-align: justify;
}
.view-summary {
  margin-bottom: 30px;
}
.sidebar ul li a.active {
  background-color: #212124 !important;
}
.table-action tr td a span {
  padding: 5px 10px;
  display: inline-block;
}
.product-basket {
  background-color: #fff;
  padding: 10px 15px 15px;
  box-shadow: 1px 2px 11px #ddd;
  /*border: 1px solid #ddd;*/
  /*border-radius: 6px;*/
}
.basket-content h3 {
  margin: 0;
}
.basket-content h3.heading {
  margin: 0;
  padding-bottom: 5px;
  border-bottom: 1px solid #D0D0D0;
  margin-bottom: 10px;
}
.basket-list {
  margin-bottom: 15px;
  border-bottom: 1px solid #D0D0D0;
}
.basket-list .lists li {
  margin-bottom: 5px;
}
.basket-list .lists li button {
  margin-right: 5px;
  border-radius: 20px;
  width: 22px;
  height: 22px;
  padding: 0;
}
.basket-list .lists li button:focus {
  outline: 0;
}
.total-wrapper {
  margin-bottom: 20px;
}
/* Custom CSS */
.tab-container .panel .panel-heading h4 {
  margin: 0;
  /*color: @cgray;*/
}
.inline-block {
  display: inline-block;
}
a {
  text-decoration: none !important;
}
a label.label {
  cursor: pointer;
}
.navbar-default {
  background-color: #262B2D;
  color: #f8f8f8;
}
.navbar-default .navbar-brand {
  color: #f8f8f8;
}